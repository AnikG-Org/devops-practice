pipeline {
    agent any
    stages {
        stage('---clean---') {
            steps {
		sh "cd maven/my-app/ && ls"
                sh "mvn clean"
            }
        }
        stage('--test--') {
            steps {
                sh "mvn test"
            }
        }
        stage('--package--') {
            steps {
                sh "mvn package"
            }
        }
    }
}
